{
    "name": "gatewayjs",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://gateway-js.herokuapp.com",
    "scripts": {
        "lint:unused-exports": "ts-unused-exports ./tsconfig.json",
        "lint:outdated-imports": "ncu",
        "lint:ts": "tslint --project ./ -t stylish --quiet",
        "lint:extra": "$npm_execpath run --silent lint:unused-exports; $npm_execpath run --silent lint:outdated-imports",
        "lint:all": "$npm_execpath run --silent lint:ts && $npm_execpath run --silent lint:extra",
        "lint": "$npm_execpath run --silent lint:ts",
        "unused-exports": "ts-unused-exports ./tsconfig.json src/**/*.ts src/**/*.tsx",
        "start": "react-scripts start",
        "build": "NODE_OPTIONS=--max_old_space_size=4096 REACT_APP_SOURCE_VERSION=$SOURCE_VERSION react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "$npm_execpath run build",
        "deploy": "gh-pages -d build"
    },
    "dependencies": {
        "@renproject/fonts": "^1.0.1",
        "@renproject/react-components": "^1.0.37",
        "@renproject/ren": "git+https://github.com/renproject/renvm-sdk-js.git#f9f495826c3a7b3565678a32c9e613287aec0c86",
        "@sentry/browser": "^5.9.1",
        "@sentry/core": "^5.8.0",
        "@sentry/integrations": "^5.8.0",
        "@types/bchaddrjs": "^0.4.0",
        "@types/bn.js": "^4.11.5",
        "@types/node": "^12.12.14",
        "@types/qrcode.react": "^1.0.0",
        "@types/qs": "^6.9.0",
        "@types/react": "^16.9.13",
        "@types/react-circular-progressbar": "^1.1.0",
        "@types/react-copy-to-clipboard": "^4.3.0",
        "@types/react-dom": "^16.9.4",
        "@types/react-router-dom": "^5.1.3",
        "@types/recharts": "^1.8.3",
        "axios": "^0.19.0",
        "bchaddrjs": "^0.4.4",
        "bignumber.js": "^9.0.0",
        "bn.js": "^5.0.0",
        "bootstrap": "^4.4.0",
        "history": "^4.10.1",
        "immutable": "^4.0.0-rc.12",
        "localforage": "^1.7.3",
        "moment": "^2.24.0",
        "qrcode.react": "^1.0.0",
        "qs": "^6.9.1",
        "react": "^16.12.0",
        "react-bootstrap": "^1.0.0-beta.9",
        "react-circular-progressbar": "^2.0.3",
        "react-copy-to-clipboard": "^5.0.2",
        "react-dom": "^16.12.0",
        "react-router-dom": "^5.1.2",
        "recharts": "^1.8.5",
        "svgo": "1.3.2",
        "unstated": "^2.1.1",
        "unstated-persist": "^0.0.4",
        "use-persisted-state": "^0.3.0",
        "wallet-address-validator": "^0.2.4",
        "web3": "^2.0.0-alpha.1"
    },
    "devDependencies": {
        "gh-pages": "^2.1.1",
        "ncu": "^0.2.1",
        "node-sass": "^4.13.0",
        "npm-check-updates": "^3.2.2",
        "react-scripts": "^3.2.0",
        "ts-unused-exports": "^5.1.0",
        "tslint": "^5.20.1",
        "tslint-microsoft-contrib": "^6.2.0",
        "tslint-react": "^4.1.0",
        "typescript": "^3.7.2"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
